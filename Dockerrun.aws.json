{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "mongo",
      "image": "mongo",
      "essential": true,
      "memoryReservation": 128
    },
    {
      "name": "wiki",
      "image": "requarks/wiki:master",
      "essential": true,
      "memoryReservation": 128,
	  "links": [
        "mongo"
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000
        }
      ]
    }
  ]
}
