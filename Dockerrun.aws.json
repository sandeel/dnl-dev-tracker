{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "requarks/wiki:master",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "80"
    },
    {
      "ContainerPort": "443"
    }
  ]
}

{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "mongo",
      "image": "mongo",
      "essential": true,
    },
    {
      "name": "wiki",
      "image": "requarks/wiki:master",
      "essential": true,
	  "links": [
        "mongo"
      ]
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
    }
  ]
}
